# Microsoft Fabric / Azure AD Configuration
# Obtain these values from Azure Portal > Azure Active Directory > App Registrations

# Azure AD Tenant ID
FABRIC_TENANT_ID=your-tenant-id-here

# Azure AD Application (Client) ID
FABRIC_CLIENT_ID=your-client-id-here

# Azure AD Application Client Secret (if using confidential client flow)
FABRIC_CLIENT_SECRET=your-client-secret-here

# Fabric Workspace Configuration
FABRIC_WORKSPACE_ID=your-workspace-id-here
FABRIC_WORKSPACE_NAME=your-workspace-name-here

# Fabric Lakehouse Configuration
FABRIC_LAKEHOUSE_ID=your-lakehouse-id-here
FABRIC_LAKEHOUSE_NAME=your-lakehouse-name-here

# Development Settings
# Set to 'True' to skip login page (development only)
DEV_MODE_SKIP_LOGIN=False

# Mock User Credentials (for development/testing only - remove in production)
MOCK_USER1_PASSWORD=your-user1-password
MOCK_ADMIN_PASSWORD=your-admin-password
